{
  "name": "bricr-spa",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "build": "react-scripts build --max-old-space-size=3072",
    "build:ci": "cross-env REACT_APP_CI=1 react-scripts build --max-old-space-size=3072",
    "coverage": "npm rebuild && cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{ts,tsx} && echo 'lint success'",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx} && echo 'lint success'",
    "start": "react-scripts start",
    "dev-port": "PORT=3006 react-scripts start",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test",
    "plop": "plop",
    "translations": "node 'scripts/get_translations.js'",
    "serve": "./node_modules/.bin/http-server ./build --port 1338 --proxy http://localhost:1338?",
    "generate:api-types": "graphql-codegen & npm run lint:fix",
    "e2e:open": "cd e2e && npx cypress open && cd ..",
    "e2e:run:firefox": "cd e2e && ./node_modules/.bin/cypress run --browser firefox && cd ..",
    "e2e:run:chrome": "cd e2e && ./node_modules/.bin/cypress run --browser chrome && cd ..",
    "e2e:wait-and-run:firefox": "./node_modules/.bin/wait-on http://localhost:1338/ && npm run e2e:run:firefox",
    "e2e:wait-and-run:chrome": "./node_modules/.bin/wait-on http://localhost:1338/ && npm run e2e:run:chrome",
    "e2e:ci:firefox": "npm-run-all -p -r serve e2e:wait-and-run:firefox",
    "e2e:ci:chrome": "npm-run-all -p -r serve e2e:wait-and-run:chrome",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "styleguidist:serve": "styleguidist server",
    "styleguidist:build": "styleguidist build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "auto-changelog": {
    "issueUrl": "https://headstart.atlassian.net/browse/{id}",
    "issuePattern": "[A-Z]+-[1-9]+"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,pcss,scss}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.44",
    "@apollo/react-common": "3.1.4",
    "@apollo/react-hooks": "3.1.5",
    "@babel/polyfill": "7.8.7",
    "@date-io/luxon": "1.3.13",
    "@devexpress/dx-react-chart": "2.7.2",
    "@devexpress/dx-react-chart-material-ui": "2.7.2",
    "@devexpress/dx-react-core": "2.7.2",
